{
  "name": "Page Matrix",
  "description": "Turn this page into matrix",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Turn this page into matrix",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs", "<all_urls>"
  ],
  "manifest_version": 2
}
